# Supabase Configuration
# These variables are exposed to the browser and should be public-safe
NEXT_PUBLIC_SUPABASE_URL="your-supabase-project-url"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"

# Server-side only variables (not exposed to browser)
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"
SUPABASE_JWT_SECRET="your-jwt-secret"

# Application Configuration
NODE_ENV="development"
PORT=3000

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_DEBUG_MODE=false

# Add any other environment variables your application needs below
# Note: Variables without NEXT_PUBLIC_ prefix are only available server-side
# Note: Keep sensitive information in server-side only variables 